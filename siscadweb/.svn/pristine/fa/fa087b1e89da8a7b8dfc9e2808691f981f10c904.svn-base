package pe.edu.sistemas.siscadweb.entities;

import java.util.ArrayList;

public class CursoBase {
	private int cursob_codigo;
	private String cursob_nombre;
	private int plan_id_plan;
	private int cursob_ciclo;
	private int cursob_creditos;
	private int codComun;
	private ArrayList<CursoConjunto> cursosConjuntos = new ArrayList<CursoConjunto>();

	public CursoBase() {
		super();
	}

	public CursoBase(int cursob_codigo, String cursob_nombre, int plan_id_plan,
			int cursob_ciclo, int cursob_creditos, int codComun) {
		super();
		this.cursob_codigo = cursob_codigo;
		this.cursob_nombre = cursob_nombre;
		this.plan_id_plan = plan_id_plan;
		this.cursob_ciclo = cursob_ciclo;
		this.cursob_creditos = cursob_creditos;
		this.codComun = codComun;
	}

	public CursoBase(int cursob_codigo, String cursob_nombre, int plan_id_plan,
			int cursob_ciclo, int cursob_creditos,
			ArrayList<CursoConjunto> cursosConjuntos) {
		super();
		this.cursob_codigo = cursob_codigo;
		this.cursob_nombre = cursob_nombre;
		this.plan_id_plan = plan_id_plan;
		this.cursob_ciclo = cursob_ciclo;
		this.cursob_creditos = cursob_creditos;
		this.cursosConjuntos = cursosConjuntos;
	}

	public int getCursob_codigo() {
		return cursob_codigo;
	}

	public int getCodComun() {
		return codComun;
	}

	public void setCodComun(int codComun) {
		this.codComun = codComun;
	}

	public void setCursob_codigo(int cursob_codigo) {
		this.cursob_codigo = cursob_codigo;
	}

	public String getCursob_nombre() {
		return cursob_nombre;
	}

	public void setCursob_nombre(String cursob_nombre) {
		this.cursob_nombre = cursob_nombre;
	}

	public int getPlan_id_plan() {
		return plan_id_plan;
	}

	public void setPlan_id_plan(int plan_id_plan) {
		this.plan_id_plan = plan_id_plan;
	}

	public int getCursob_ciclo() {
		return cursob_ciclo;
	}

	public void setCursob_ciclo(int cursob_ciclo) {
		this.cursob_ciclo = cursob_ciclo;
	}

	public int getCursob_creditos() {
		return cursob_creditos;
	}

	public void setCursob_creditos(int cursob_creditos) {
		this.cursob_creditos = cursob_creditos;
	}

	public ArrayList<CursoConjunto> getCursosConjuntos() {
		return cursosConjuntos;
	}

	public void setCursosConjuntos(ArrayList<CursoConjunto> cursosConjuntos) {
		this.cursosConjuntos = cursosConjuntos;
	}
}
